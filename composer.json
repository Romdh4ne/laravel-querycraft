{
    "name": "romdh4ne/laravel-querycraft",
    "description": "Find and fix Laravel performance issues automatically",
    "keywords": [
        "laravel",
        "performance",
        "database",
        "queries",
        "n-plus-one",
        "optimization"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Romdhane",
            "email": "med.romdhane06@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support":  "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console":  "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit":     "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Romdh4ne\\QueryCraft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Romdh4ne\\QueryCraft\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Romdh4ne\\QueryCraft\\QueryCraftServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}